var checkOut = HttpContext.Request.Query["endDate"];

}
<h1>Search Results</h1>
@Html.Raw(Model.JsonData)
<div id="hotelResults" style="visibility:hidden;">

</div>

<script type="module">
displayHotelResults(@lat, @longitude, @checkIn, @checkOut);
</script>